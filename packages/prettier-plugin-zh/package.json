{
  "name": "prettier-plugin-zh",
  "version": "0.5.2",
  "description": "Prettier plugin for formatting Zh(Chinese) text.",
  "keywords": [
    "prettier",
    "prettier-plugin",
    "chinese"
  ],
  "homepage": "",
  "repository": "https://github.com/nnecec/zh",
